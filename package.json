{
  "name": "@grace-js/grace",
  "description": "An opinionated framework",
  "version": "0.3.14-beta.7",
  "author": {
    "name": "Fumaz",
    "url": "https://fumaz.dev",
    "email": "mail@fumaz.dev"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "bun": "./dist/bun/index.js",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist/ && npm run build:esm && npm run build:bun && npm run build:cjs",
    "build:bun": "bun build.ts",
    "build:esm": "swc src -d dist && tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.86",
    "bun-types": "latest",
    "typescript": "^5.2.2",
    "@sentry/integrations": "^7.70.0",
    "@sentry/node": "^7.70.0"
  },
  "peerDependencies": {
    "typescript": ">= 5.0.0",
    "@sinclair/typebox": ">= 0.28.10",
    "glob": ">= 10.3.5"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.31.15",
    "fast-querystring": "^1.1.2",
    "glob": "^10.3.5",
    "openapi-types": "^12.1.3",
    "route-trie": "^3.0.0"
  }
}
